{% extends 'admin.html.twig' %}

{% block title %}Projet{% endblock %}

{% block pagetitle %}{% endblock %}

{% block body %}
    <h1>Projet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ projet.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ projet.slug }}</td>
            </tr>
            <tr>
                <th>FDescriptive</th>
                <td>{{ projet.fDescriptive }}</td>
            </tr>
            <tr>
                <th>AFiabilite</th>
                <td>{{ projet.aFiabilite }}</td>
            </tr>
            <tr>
                <th>DInfReglementaire</th>
                <td>{{ projet.dInfReglementaire }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ projet.endDate ? projet.endDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>MCollecte</th>
                <td>{{ projet.mCollecte }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ projet.created ? projet.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ projet.updated ? projet.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_projets_crud_index') }}">back to list</a>

    <a href="{{ path('app_admin_projets_crud_edit', {'id': projet.id}) }}">edit</a>

    {{ include('admin_projets_crud/_delete_form.html.twig') }}
{% endblock %}
